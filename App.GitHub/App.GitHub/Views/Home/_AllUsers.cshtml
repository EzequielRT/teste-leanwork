@model IEnumerable<App.GitHub.ViewModels.UserViewModel>

@foreach (var item in Model)
{
    <tr data-id="@item.Id">
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Login)
        </td>
        <td>
            <button class="btn btn-outline-dark btnModal" data-login="@item.Login"><span class="fa fa-search"></span></button>
        </td>
    </tr>
}